{
  "extends": "tslint:latest",
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "max-line-length": [true, 150],
    "no-console": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "no-var-requires": false,
    "ordered-imports": [false],
    "variable-name": false,
    "space-before-function-paren": false
  }
}