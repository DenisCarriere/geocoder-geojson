  "dependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/request": "0.0.31",
    "@types/request-promise": "^3.0.32",
    "bluebird": "^3.4.6",
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  }
