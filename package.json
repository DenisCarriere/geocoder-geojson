    "docs": "tsc && cat HEADER.md > README.md && documentation build index.js -c documentation.yml -f md >> README.md",
    "lint": "tsc && tslint index.ts test.ts && documentation lint index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisCarriere/geocoder-promise.git"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "contributors": [
    "Denis Carriere <carriere.denis@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisCarriere/geocoder-promise/issues"
  },
  "homepage": "https://github.com/DenisCarriere/geocoder-promise#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "documentation": "^4.0.0-beta10",
    "nyc": "^8.3.0",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "extent",
    "geojson",
    "google",
    "map",
    "geocode",
    "geocoding",
    "geocoder"
